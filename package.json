{
  "name": "select-package-flutter",
  "displayName": "Select Package Flutter",
  "description": "this package can make search for any package in pub dev flutter and install it",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "select-package-flutter.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "select-package-flutter.installDependency",
        "title": "installPackage"
      },
      {
        "command": "select-package-flutter.searchFlutterDependency",
        "title": "searchPackage"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.5"
  }
}
